<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RentwizTextview">
        <attr name="rentwizTextType" format="enum">
            <enum name="h1" value="0" />
            <enum name="h2" value="1" />
            <enum name="h3" value="2" />
            <enum name="h4" value="3" />
            <enum name="h5" value="4" />
            <enum name="subtitle_1" value="5" />
            <enum name="subtitle_2" value="6" />
            <enum name="body_1" value="7" />
            <enum name="body_2" value="8" />
            <enum name="body_3" value="9" />
            <enum name="body_4" value="10" />
            <enum name="body_5" value="11" />
            <enum name="body_6" value="12" />
            <enum name="body_7" value="13" />
            <enum name="body_8" value="14" />
            <enum name="body_9" value="15" />
            <enum name="form_label" value="16" />
            <enum name="form_input_field" value="17" />
            <enum name="table" value="18" />
            <enum name="table_1" value="19" />
            <enum name="button_1" value="20" />
            <enum name="incred_card" value="21" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RentwizButton">
        <attr name="rentwizButtonType" format="enum">
            <enum name="large" value="0"/>
            <enum name="dialog_positive" value="1"/>
            <enum name="dialog_negative" value="2"/>
            <enum name="banner" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="RentwizCustomEditText">
        <attr name="icon_start" format="reference" />
        <attr name="icon_end" format="reference" />
        <attr name="right_text" format="string"/>
        <attr name="hint" format="string" />
        <attr name="view_enabled" format="boolean" />
        <attr name="underline_enabled" format="boolean" />
        <attr name="max_length" format="integer" min="0" />
        <attr name="input_type" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="date" value="2" />
            <enum name="textEmailAddress" value="3" />
        </attr>
        <attr name="input_digits" format="string" />
        <attr name="prefix_text" format="string" />
    </declare-styleable>
</resources>